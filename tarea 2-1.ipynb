   "source": [
    "import numpy as np\n",
    "\n",
    "def forward_propagation(x, w, b):\n",
    "    \n",
    "    y = np.dot(w, x) + b \n",
    "    return y\n",
    "\n",
    "\n",
    "x = np.array([1.9, 3.8])   \n",
    "w = np.array([2.5, -1.7])  \n",
    "b = 0.4                    \n",
    "\n",
    "salida = forward_propagation(x, w, b)\n",
    "print(\"Salida de la neurona:\", salida)\n"
   ]
  }
